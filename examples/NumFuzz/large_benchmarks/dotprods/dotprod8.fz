#include "combine8.fz"
#include "map8_prod.fz"
#include "mulfp.fz"
#include "addfp.fz"
#include "fold8.fz"


function dotprod8 
 (a: (num, (num, (num, (num, (num, (num, (num, num))))))) )
 (b: (num, (num, (num, (num, (num, (num, (num, num))))))) )
{
 v  = combine8 a b ;
 v' = map8_prod v [mulfp{8.0}];

 let (a0',as1) = v';
 let (a1',as2) = as1;
 let (a2',as3) = as2;
 let (a3',as4) = as3;
 let (a4',as5) = as4;
 let (a5',as6) = as5;
 let (a6',a7')  = as6;

 let a0 = a0';
 let a1 = a1';
 let a2 = a2';
 let a3 = a3';
 let a4 = a4';
 let a5 = a5';
 let a6 = a6';
 let a7 = a7';

 v1 = (a0,(a1,(a2,(a3,(a4,(a5,(a6,a7))))))) ; 

 g = fun (x: num) {fun (y: num) {addfp (|x,y|)}};
 fold8 [g{7.0}] v1 
}
