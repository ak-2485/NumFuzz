  c33 = dotprod8 A3 B3;
  c34 = dotprod8 A3 B4;
  c35 = dotprod8 A3 B5;
  c36 = dotprod8 A3 B6;
  c37 = dotprod8 A3 B7;

  c40 = dotprod8 A4 B0;
