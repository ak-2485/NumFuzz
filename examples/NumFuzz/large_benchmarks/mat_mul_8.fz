      ((num, (num, (num, (num, (num, (num, (num, num))))))),
      (num, (num, (num, (num, (num, (num, (num, num))))))))))))))))  
  (B : ![8.0]((num, (num, (num, (num, (num, (num, (num, num))))))),
      (((num, (num, (num, (num, (num, (num, (num, num))))))),
      ((num, (num, (num, (num, (num, (num, (num, num))))))),
      ((num, (num, (num, (num, (num, (num, (num, num))))))),
      ((num, (num, (num, (num, (num, (num, (num, num))))))),
      ((num, (num, (num, (num, (num, (num, (num, num))))))),
      ((num, (num, (num, (num, (num, (num, (num, num))))))),
      (num, (num, (num, (num, (num, (num, (num, num))))))))))))))))

{
/* rows of A */
  let [A'] = A;
  let (A0,AS1) = A';
  let (A1,AS2) = AS1;
  let (A2,AS3) = AS2;
  let (A3,AS4) = AS3;
  let (A4,AS5) = AS4;
  let (A5,AS6) = AS5;
  let (A6,A7)  = AS6;

/* columns of B */
  let [B'] = B;
  let (B0,BS1) = B';
  let (B1,BS2) = BS1;
  let (B2,BS3) = BS2;
  let (B3,BS4) = BS3;
  let (B4,BS5) = BS4;
  let (B5,BS6) = BS5;
  let (B6,B7)  = BS6;

/* cij is the dot product of the ith row of A and the jth column of B */

  c00 = dotprod8 A0 B0;
  c01 = dotprod8 A0 B1;
  c02 = dotprod8 A0 B2;
  c03 = dotprod8 A0 B3;
  c04 = dotprod8 A0 B4;
