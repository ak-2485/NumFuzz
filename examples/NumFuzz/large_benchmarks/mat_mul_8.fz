  c14 = dotprod8 A1 B4;
  c15 = dotprod8 A1 B5;
  c16 = dotprod8 A1 B6;
  c17 = dotprod8 A1 B7;

  c20 = dotprod8 A2 B0;
  c21 = dotprod8 A2 B1;
  c22 = dotprod8 A2 B2;
  c23 = dotprod8 A2 B3;
  c24 = dotprod8 A2 B4;
  c25 = dotprod8 A2 B5;
  c26 = dotprod8 A2 B6;
  c27 = dotprod8 A2 B7;

  c30 = dotprod8 A3 B0;
  c31 = dotprod8 A3 B1;
