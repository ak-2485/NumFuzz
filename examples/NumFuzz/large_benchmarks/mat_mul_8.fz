  c42 = dotprod8 A4 B2;
  c43 = dotprod8 A4 B3;
  c44 = dotprod8 A4 B4;
  c45 = dotprod8 A4 B5;
  c46 = dotprod8 A4 B6;
  c47 = dotprod8 A4 B7;

  c50 = dotprod8 A5 B0;
  c51 = dotprod8 A5 B1;
  c52 = dotprod8 A5 B2;
  c53 = dotprod8 A5 B3;
  c54 = dotprod8 A5 B4;
  c55 = dotprod8 A5 B5;
  c56 = dotprod8 A5 B6;
  c57 = dotprod8 A5 B7;

  c60 = dotprod8 A6 B0;
  c61 = dotprod8 A6 B1;
  c62 = dotprod8 A6 B2;
  c63 = dotprod8 A6 B3;
  c64 = dotprod8 A6 B4;
  c65 = dotprod8 A6 B5;
  c66 = dotprod8 A6 B6;
  c67 = dotprod8 A6 B7;

  c70 = dotprod8 A7 B0;
