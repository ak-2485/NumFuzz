
#include "dotprods/dotprod8.fz"


function mat_mul_8
  (A : ![8.0]((num, (num, (num, (num, (num, (num, (num, num))))))),
      (((num, (num, (num, (num, (num, (num, (num, num))))))),
      ((num, (num, (num, (num, (num, (num, (num, num))))))),
      ((num, (num, (num, (num, (num, (num, (num, num))))))),
